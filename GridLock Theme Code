{/block:ifManualLoadInfiniteScroll}

<script>
$(document).ready(function(){// hide #back-top first
    $("#back-top").hide();
    $(".sticky").hide();

    // fade in #back-top
    $(function () {
        $(window).scroll(function () {
        if ($(this).scrollTop() > 100) {
        $('#back-top').fadeIn();
        } else {

       $('#back-top').fadeOut();
        }
        });

        // scroll body to 0px on click
        $('#back-top a').click(function () {
        $('body,html').animate({
        scrollTop: 0
        }, 800);
        return false;
        });
    });
});

$(window).scroll(function() {
if ($(this).scrollTop() > 150){  
    $('.sticky').fadeIn();
     }  else {

     $('.sticky').fadeOut();
     }
    });
</script>


<script>
$(function(){
    var $container = $('#container');
    $container.imagesLoaded(function(){
    $container.masonry({
    gutter:50,
    columnWidth: 10,
    itemSelector: '#post',
    //isAnimated: true,
    //animationOptions: {
        //duration: 750,
        //easing: 'linear',
        //queue: false
        //}
    });
    });

$container.infinitescroll({
    itemSelector : "#post",
    navSelector : "div.navigation",
    nextSelector : "div.navigation a#next",
    loading:{
        img : "https://portal.ehawaii.gov/assets/images/loading.gif",
        finishedMsg: "All post have been loaded.",
        msgText : "Loading more post.",
    },
    maxPage: 10,
    behavior: 'twitter'
//    donetext: "All post has been loaded.",
//    bufferPx : 40,
//    extraScrollPx: 150,
//    preload : false,
},
// trigger Masonry as a callback
function( newElements ) {
  var $newElems = $( newElements ).css({ opacity: 0 });
// ensure that images load before adding to masonry layout
    $newElems.imagesLoaded(function(){
    $newElems.animate({ opacity: 1 });
    $container.masonry( 'appended', $newElems, true ); 
    });
    }
);
{block:ifManualLoadInfiniteScroll}
$(window).unbind('.infscr');
$('a#next').click(function(){	
	$(document).trigger('retrieve.infscr');return false;});
	$(document).ajaxError(function(e,xhr,opt){if(xhr.status==404)$('a#next').remove();});
{/block:ifManualLoadInfiniteScroll}
});
</script>

